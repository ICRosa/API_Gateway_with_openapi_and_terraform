
variable "pg_port" {
  description = "PostgreSQL port"
  type        = number
}

variable "pg_user" {
  description = "PostgreSQL user"
  type        = string
}

variable "pg_pass" {
  description = "PostgresSQL password"
  type        = string
}

variable "pg_dbname" {
  description = "PostgreSQL database name"
  type        = string
}